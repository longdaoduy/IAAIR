{
  "dashboard": {
    "id": null,
    "title": "IAAIR Performance Dashboard",
    "tags": ["iaair", "performance", "search"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(iaair_requests_total[1m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Active Requests",
        "type": "stat",
        "targets": [
          {
            "expr": "iaair_active_requests",
            "legendFormat": "Active"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Response Time (95th percentile)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(iaair_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(iaair_errors_total[1m])",
            "legendFormat": "Errors/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "min": 0,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.01},
                {"color": "red", "value": 0.1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Response Time Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(iaair_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(iaair_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(iaair_request_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (seconds)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Component Latency Breakdown",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(iaair_embedding_duration_seconds_bucket[5m]))",
            "legendFormat": "Embedding"
          },
          {
            "expr": "histogram_quantile(0.95, rate(iaair_vector_search_duration_seconds_bucket[5m]))",
            "legendFormat": "Vector Search"
          },
          {
            "expr": "histogram_quantile(0.95, rate(iaair_graph_search_duration_seconds_bucket[5m]))",
            "legendFormat": "Graph Search"
          },
          {
            "expr": "histogram_quantile(0.95, rate(iaair_reranking_duration_seconds_bucket[5m]))",
            "legendFormat": "Reranking"
          },
          {
            "expr": "histogram_quantile(0.95, rate(iaair_ai_response_duration_seconds_bucket[5m]))",
            "legendFormat": "AI Response"
          }
        ],
        "yAxes": [
          {
            "label": "Latency (seconds)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Cache Hit Rates",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(iaair_cache_hits_total{cache_type=\"embedding\"}[5m]) / (rate(iaair_cache_hits_total{cache_type=\"embedding\"}[5m]) + rate(iaair_cache_misses_total{cache_type=\"embedding\"}[5m]))",
            "legendFormat": "Embedding Cache"
          },
          {
            "expr": "rate(iaair_cache_hits_total{cache_type=\"search\"}[5m]) / (rate(iaair_cache_hits_total{cache_type=\"search\"}[5m]) + rate(iaair_cache_misses_total{cache_type=\"search\"}[5m]))",
            "legendFormat": "Search Cache"
          },
          {
            "expr": "rate(iaair_cache_hits_total{cache_type=\"ai_response\"}[5m]) / (rate(iaair_cache_hits_total{cache_type=\"ai_response\"}[5m]) + rate(iaair_cache_misses_total{cache_type=\"ai_response\"}[5m]))",
            "legendFormat": "AI Response Cache"
          }
        ],
        "yAxes": [
          {
            "label": "Hit Rate",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Routing Strategy Usage",
        "type": "piechart",
        "targets": [
          {
            "expr": "increase(iaair_requests_total[1h])",
            "legendFormat": "{{routing_strategy}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 9,
        "title": "Cache Sizes",
        "type": "graph",
        "targets": [
          {
            "expr": "iaair_cache_size{cache_type=\"embedding_cache\"}",
            "legendFormat": "Embedding Cache"
          },
          {
            "expr": "iaair_cache_size{cache_type=\"search_cache\"}",
            "legendFormat": "Search Cache"
          },
          {
            "expr": "iaair_cache_size{cache_type=\"ai_response_cache\"}",
            "legendFormat": "AI Response Cache"
          }
        ],
        "yAxes": [
          {
            "label": "Cache Entries",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 10,
        "title": "Results Count Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(iaair_results_count_bucket[5m]))",
            "legendFormat": "Median Results"
          },
          {
            "expr": "histogram_quantile(0.95, rate(iaair_results_count_bucket[5m]))",
            "legendFormat": "95th Percentile Results"
          }
        ],
        "yAxes": [
          {
            "label": "Number of Results",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ]
  }
}